{
  "cli": {
    "version": ">= 9.1.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_APPWRITE_PROJECT_ID": "${EXPO_PUBLIC_APPWRITE_PROJECT_ID}",
